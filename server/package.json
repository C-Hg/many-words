{
  "name": "server",
  "version": "1.0.0",
  "description": "Many-words server",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d lib",
    "check-types": "tsc",
    "deploy": "npm run build && npm run serve",
    "generate": "graphql-codegen --config codegen.yml && eslint --fix --quiet src/graphql/authorization.types.tsx src/graphql/exercises.types.tsx",
    "serve": "node lib/server.js",
    "start": "nodemon --exec babel-node --extensions '.ts' src/server.ts",
    "test:ci": "jest",
    "test:local": "jest --coverage  || true"
  },
  "author": "Camille HAGENBOURGER",
  "license": "MIT",
  "dependencies": {
