    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.sample": "^4.2.1",
    "lodash.set": "^4.3.2",
    "mongodb": "^4.11.0",
    "mongoose": "^6.6.7",
    "validator": "^13.5.2",
