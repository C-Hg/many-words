{
  "name": "server",
  "version": "1.0.0",
  "description": "Many-words server",
  "main": "server.js",
  "scripts": {
    "build": "babel src -d lib",
    "check-types": "tsc",
    "deploy": "npm run build && npm run serve",
    "lint:graphql": "eslint --fix --quiet src/graphql/types.tsx",
    "serve": "node lib/server.js",
    "start": "npm i && nodemon --exec babel-node --extensions '.ts' src/server.ts",
    "start-and-test": "npm start && jest --watch",
    "test:ci": "jest",
    "test:local": "jest --coverage  || true"
  },
  "author": "Camille HAGENBOURGER",
  "license": "MIT",
  "dependencies": {
