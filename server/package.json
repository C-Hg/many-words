    "apollo-server-testing": "^2.12.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.0",
    "eslint-config-prettier": "^6.10.1",
