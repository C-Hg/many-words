    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lodash.sample": "^4.2.1",
    "lodash.set": "^4.3.2",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.15",
    "validator": "^13.1.17",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@apollo/client": "^3.2.7",
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.7",
